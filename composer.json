{
    "name": "urbanetter/jassy",
    "description": "An evolving AI for a Swiss card game called Jass",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9",
        "vimeo/psalm": "^4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Urban Etter",
            "email": "uetter@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jass\\": "src/"
        },
        "files": [
            "src/CardSetFunctions.php",
            "src/HandFunctions.php",
            "src/TrickFunctions.php",
            "src/GameFunctions.php",
            "src/StrategyFunctions.php",
            "src/PlayerFunctions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1"
    }
}
